function bash_colors() {
    # Reset
    Color_Off='\033[0m'       # Text Reset

    # echo -e '\e[30mBlack \e[0m \t \e[40mBlack \e[0m'
    # echo -e '\e[31mRed   \e[0m \t \e[41mRed   \e[0m'
    # echo -e '\e[32mGreen \e[0m \t \e[42mGreen \e[0m'
    # echo -e '\e[33mYellow\e[0m \t \e[43mYellow\e[0m'
    # echo -e '\e[34mBlue  \e[0m \t \e[44mBlue  \e[0m'
    # echo -e '\e[35mPurple\e[0m \t \e[45mPurple\e[0m'
    # echo -e '\e[36mCyan  \e[0m \t \e[46mCyan  \e[0m'
    # echo -e '\e[37mWhite \e[0m \t \e[47mWhite \e[0m'

    # echo -e '\033[0;30m Black \e[0m'
    # echo -e '\033[0;31m Red \e[0m'
    # echo -e '\033[0;32m Green \e[0m'
    # echo -e '\033[0;33m Yellow \e[0m'
    # echo -e '\033[0;34m Blue \e[0m'
    # echo -e '\033[0;35m Purple \e[0m'
    # echo -e '\033[0;36m Cyan \e[0m'
    # echo -e '\033[0;37m White \e[0m'


    echo -e '\033[0;30m Black  \e[0m \t \033[1;30m BBlack  \e[0m \t \033[4;30m UBlack  \e[0m \t \033[40m OnBlack \e[0m'
    echo -e '\033[0;31m Red    \e[0m \t \033[1;31m BRed    \e[0m \t \033[4;31m URed    \e[0m \t \033[41m OnRed \e[0m'

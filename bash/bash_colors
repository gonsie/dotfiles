    # # Bold
    # echo -e '\033[1;30m BBlack \e[0m'
    # echo -e '\033[1;31m BRed \e[0m'
    # echo -e '\033[1;32m BGreen \e[0m'
    # echo -e '\033[1;33m BYellow \e[0m'
    # echo -e '\033[1;34m BBlue \e[0m'
    # echo -e '\033[1;35m BPurple \e[0m'
    # echo -e '\033[1;36m BCyan \e[0m'
    # echo -e '\033[1;37m BWhite \e[0m'
    # # Underline
    # echo -e '\033[4;30m UBlack \e[0m'
    # echo -e '\033[4;31m URed \e[0m'
    # echo -e '\033[4;32m UGreen \e[0m'
    # echo -e '\033[4;33m UYellow \e[0m'
    # echo -e '\033[4;34m UBlue \e[0m'
    # echo -e '\033[4;35m UPurple \e[0m'
    # echo -e '\033[4;36m UCyan \e[0m'
    # echo -e '\033[4;37m UWhite \e[0m'
    # # Background
    # echo -e '\033[40m OnBlack \e[0m'
    # echo -e '\033[41m OnRed \e[0m'
    # echo -e '\033[42m OnGreen \e[0m'
    # echo -e '\033[43m OnYellow \e[0m'
    # echo -e '\033[44m OnBlue \e[0m'
    # echo -e '\033[45m OnPurple \e[0m'
    # echo -e '\033[46m OnCyan \e[0m'
    # echo -e '\033[47m OnWhite \e[0m'
    # # High Intensity
    # echo -e '\033[0;90m IBlack \e[0m'
    # echo -e '\033[0;91m IRed \e[0m'
    # echo -e '\033[0;92m IGreen \e[0m'
    # echo -e '\033[0;93m IYellow \e[0m'
    # echo -e '\033[0;94m IBlue \e[0m'

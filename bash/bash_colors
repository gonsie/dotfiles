    # echo -e '\033[1;94m BIBlue \e[0m'
    # echo -e '\033[1;95m BIPurple \e[0m'
    # echo -e '\033[1;96m BICyan \e[0m'
    # echo -e '\033[1;97m BIWhite \e[0m'
    # # High Intensity backgrounds

    echo -e '\033[0;33m Yellow \e[0m \t \033[1;33m BYellow \e[0m \t \033[4;33m UYellow \e[0m \t \033[43m OnYellow \e[0m'
    echo -e '\033[0;34m Blue   \e[0m \t \033[1;34m BBlue   \e[0m \t \033[4;34m UBlue   \e[0m \t \033[44m OnBlue \e[0m'
    echo -e '\033[0;35m Purple \e[0m \t \033[1;35m BPurple \e[0m \t \033[4;35m UPurple \e[0m \t \033[45m OnPurple \e[0m'
    echo -e '\033[0;36m Cyan   \e[0m \t \033[1;36m BCyan   \e[0m \t \033[4;36m UCyan   \e[0m \t \033[46m OnCyan \e[0m'

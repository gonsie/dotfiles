    # echo -e '\033[0;96m ICyan \e[0m'
    # echo -e '\033[0;97m IWhite \e[0m'
    # High Intensity
    echo -e '\033[0;90m IBlack  \e[0m \t \033[1;90m BIBlack  \e[0m \t \033[0;100m OnIBlack \e[0m'
    echo -e '\033[0;91m IRed    \e[0m \t \033[1;91m BIRed    \e[0m \t \033[0;101m OnIRed \e[0m'
    echo -e '\033[0;92m IGreen  \e[0m \t \033[1;92m BIGreen  \e[0m \t \033[0;102m OnIGreen \e[0m'
    echo -e '\033[0;93m IYellow \e[0m \t \033[1;93m BIYellow \e[0m \t \033[0;103m OnIYellow \e[0m'
    echo -e '\033[0;94m IBlue   \e[0m \t \033[1;94m BIBlue   \e[0m \t \033[0;104m OnIBlue \e[0m'
    echo -e '\033[0;95m IPurple \e[0m \t \033[1;95m BIPurple \e[0m \t \033[0;105m OnIPurple \e[0m'
    echo -e '\033[0;96m ICyan   \e[0m \t \033[1;96m BICyan   \e[0m \t \033[0;106m OnICyan \e[0m'
